<form action="" method="post" >
    <table>
        <tr>
            <th>Roll No:</th>
            <td><input type="text" name="rollno"></td>
        </tr>
        <tr>
            <th>Name:</th>
            <td><input type="text" name="name"></td>
        </tr>
        <tr>
            <th>Sub1:</th>
            <td><input type="text" name="sub1"></td>
        </tr>
        <tr>
            <th>Sub2:</th>
            <td><input type="text" name="sub2"></td>
        </tr>
        <tr>
            <th>Sub3:</th>
            <td><input type="text" name="sub3"></td>
        </tr>
       
        <tr>
            <th></th>
            <td><input type="submit" name="save"></td>
        </tr>
        
    </table>
</form>
<table border="">
    <th>ID</th>
    <th>rollno</th>
    <th>name</th>
    <th>sub1</th>
    <th>sub2</th>
    <th>sub3</th>
    <th>total</th>
    <th>per</th>
    <th>min</th>
    <th>max</th>
    <th>resl</th>
    <th>Delete</th>
    <th>Update</th>
    <% result.forEach(function(user){%>
        <tr>
            <td><%=user.id%></td>
            <td><%=user.rollno%></td>
            <td><%=user.name%></td>
            <td><%=user.sub1%></td>
            <td><%=user.sub2%></td>
            <td><%=user.sub3%></td>
            <td><%=user.total%></td>
            <td><%=user.per%></td>
            <td><%=user.min%></td>
            <td><%=user.max%></td>
            <td><%=user.resl%></td>
            <td><a href="delete/<%=user.id%>">Delete</a></td>
            <td><a href="update/<%=user.id%>">Update</a></td>
        </tr>
        <% });%>
</table>